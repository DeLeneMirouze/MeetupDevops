@using MeetupWeb.Home;
@model MeetupWeb.Home.Models.HomeDto;

@{
    ViewData["Title"] = @Model?.Titre;
}

<h1 class="text-center display-4">@Model?.Titre</h1>
<p>
<ul class="list-group">
        @foreach (var line in Model.Lines)
        {
        <li class="list-group-item list-group-item-action">@line</li>
        }
</ul>
</p>
